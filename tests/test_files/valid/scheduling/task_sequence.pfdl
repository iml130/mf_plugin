Location warehouse1
    id: "0x4711"
    type: "pallet"
End

Location production1
    id: "0x4712"
    type: "pallet"
End

TransportOrderStep tosWarehouse1
    Location warehouse1
End

TransportOrderStep tosProduction1
    Location production1
End

Task mainTask
    Transport
    From tosWarehouse1
    To tosProduction1
    helloWorld
End

Task helloWorld
    Transport
    From tosWarehouse1
    To tosProduction1

    helloWorld2
End

Task helloWorld2
    Transport
    From tosWarehouse1
    To tosProduction1
End
