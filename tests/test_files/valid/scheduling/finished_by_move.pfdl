Location warehouse1
    id: "0x4711"
    type: "pallet"
End

Location production1
    id: "0x4712"
    type: "pallet"
End

Event manualLoadingConfirmation
    id : "buttonFtfIsLoaded"
    value : false
End

TransportOrderStep tosWarehouse1
    Location warehouse1
End

MoveOrderStep mosWarehouse1
    Location warehouse1
End

TransportOrderStep tosProduction1
    Location production1
End

Task mainTask
    helloWorld
End

Task helloWorld
    Transport
    From tosWarehouse1
    To tosProduction1

    Move 
    To mosWarehouse1

    FinishedBy manualLoadingConfirmation.value
End
